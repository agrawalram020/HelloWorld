pipeline {
   agent any

   stages {
      stage('Hello') {
         steps {
            bat 'pushd dev
            pushd %workbench_name%'
            bat 'echo %tfvars%>>dev/%workbench_name%/test.tfvars'
            bat 'echo line3_in_vm_name'
            bat 'echo %workbench_name%'
            bat 'python demo.py test.tfvars'
         }
      }
   }
}
